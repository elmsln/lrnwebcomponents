{
  "name": "@lrnwebcomponents/lrs-service",
  "description": "Learning Record Store Service",
  "version": "0.0.0",
  "author": "heyMP <mgp140@gmail.com>",
  "bin": {
    "lrs-service": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/elmsln/lrnwebcomponents/issues"
  },
  "dependencies": {
    "apollo-server": "3.12.0",
    "graphql": "16.7.0",
    "graphql-type-json": "0.3.2",
    "prisma-client-lib": "1.34.12",
    "nexus": "1.3.0",
    "nexus-prisma": "1.0.8"
  },
  "devDependencies": {
    "nodemon": "2.0.22",
    "patch-package": "7.0.0",
    "postinstall-postinstall": "2.1.0",
    "@oclif/command": "1.8.28",
    "@oclif/config": "1.18.10",
    "@oclif/plugin-help": "5.2.10"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "lib"
  ],
  "homepage": "https://github.com/elmsln/lrnwebcomponents#readme",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lrs-service.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "lrs-service",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elmsln/lrnwebcomponents.git"
  },
  "scripts": {
    "start": "lrs-service start",
    "dev": "nodemon lib/lrs-service.js",
    "postinstall": "patch-package",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
