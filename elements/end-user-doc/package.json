{
  "name": "@lrnwebcomponents/end-user-doc",
  "wcfactory": {
    "className": "EndUserDoc",
    "customElementClass": "LitElement",
    "elementName": "end-user-doc",
    "generator-wcfactory-version": "0.10.0",
    "useHAX": false,
    "useSass": false,
    "files": {
      "js": "src/end-user-doc.js"
    },
    "iconset": {},
    "sharedHaxProps": [],
    "sharedProps": [],
    "sharedStyles": []
  },
  "version": "4.0.9",
  "description": "given an array of feature documentation, will generate end user documentation (good for modular, customizable tools where documentation whould be based on what is enabled or added on)",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "end-user-doc.js",
  "module": "end-user-doc.js",
  "umd": "end-user-doc.umd.js",
  "scripts": {
    "test": "wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/end-user-doc/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp --gulpfile=gulpfile.cjs && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}' && wca analyze \"**/*.js\" --outFile custom-elements.json",
    "dev": "../../node_modules/.bin/concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev --gulpfile=gulpfile.cjs",
    "serve": "web-dev-server -c ../../web-dev-server.config.js",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse --gulpfile=gulpfile.cjs"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "lit-element": "^2.3.1"
  },
  "devDependencies": {
    "web-animations-js": "2.3.2",
    "@lrnwebcomponents/deduping-fix": "2.6.16",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@webcomponents/webcomponentsjs": "2.4.4",
    "gulp-babel": "8.0.0",
    "lodash": "4.17.19",
    "polymer-cli": "1.9.11",
    "es-dev-server": "1.57.4",
    "concurrently": "5.0.2",
    "polymer-build": "3.1.4",
    "wct-browser-legacy": "1.0.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
