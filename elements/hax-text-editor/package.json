{
  "name": "@lrnwebcomponents/hax-text-editor",
  "wcfactory": {
    "className": "HaxTextEditor",
    "customElementClass": "PolymerElement",
    "elementName": "hax-text-editor",
    "generator-wcfactory-version": "0.7.1",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/hax-text-editor.css",
      "scss": "src/hax-text-editor.scss",
      "html": "src/hax-text-editor.html",
      "js": "src/hax-text-editor.js",
      "properties": "src/hax-text-editor-properties.json",
      "hax": "src/hax-text-editor-hax.json"
    }
  },
  "version": "2.1.3",
  "description": "rich-text-editor configured for HAX",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "hax-text-editor.js",
  "module": "hax-text-editor.es6.js",
  "umd": "hax-text-editor.umd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/hax-text-editor/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/lrn-math": "^2.1.2",
    "@lrnwebcomponents/lrn-vocab": "^2.1.3",
    "@lrnwebcomponents/oer-schema": "^2.1.2",
    "@lrnwebcomponents/rich-text-editor": "^2.1.3",
    "@polymer/polymer": "^3.0.5"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "2.0.5",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "concurrently": "4.1.1",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "lodash": "4.17.15",
    "polymer-build": "3.1.4",
    "polymer-cli": "1.9.11",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
