{
  "name": "@lrnwebcomponents/rich-text-editor",
  "wcfactory": {
    "className": "RichTextEditor",
    "customElementClass": "LitElement",
    "elementName": "rich-text-editor",
    "generator-wcfactory-version": "0.6.5",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/rich-text-editor.css",
      "scss": "src/rich-text-editor.scss",
      "html": "src/rich-text-editor.html",
      "js": "src/rich-text-editor.js",
      "properties": "src/rich-text-editor-properties.json",
      "hax": "src/rich-text-editor-hax.json"
    },
    "sharedStyles": [
      "...super.styles"
    ]
  },
  "version": "2.7.10",
  "description": "a standalone rich text editor",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "type": "module",
  "main": "rich-text-editor.js",
  "module": "rich-text-editor.js",
  "umd": "rich-text-editor.umd.js",
  "scripts": {
    "test": "wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/rich-text-editor/test/",
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && rollup -c && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\" && wca analyze \"**/*.js\" --format vscode --outFile vscode-html-custom-data.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "es-dev-server -c ../../es-dev-server.config.js",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/a11y-collapse": "^2.7.10",
    "@lrnwebcomponents/absolute-position-behavior": "^2.7.10",
    "@lrnwebcomponents/es-global-bridge": "^2.7.10",
    "@lrnwebcomponents/md-extra-icons": "^2.7.10",
    "@lrnwebcomponents/responsive-utility": "^2.7.10",
    "@lrnwebcomponents/simple-fields": "^2.7.10",
    "@lrnwebcomponents/simple-picker": "^2.7.10",
    "@lrnwebcomponents/simple-popover": "^2.7.10",
    "@lrnwebcomponents/simple-tooltip": "^2.7.10",
    "@polymer/iron-a11y-keys": "^3.0.1",
    "@polymer/iron-fit-behavior": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/paper-input": "^3.0.2",
    "shadow-selection-polyfill": "^1.1.0",
    "lit-element": "2.4.0"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^2.7.7",
    "@lrnwebcomponents/storybook-utilities": "^2.7.10",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "2.5.0",
    "concurrently": "5.3.0",
    "gulp-babel": "8.0.0",
    "lodash": "4.17.20",
    "polymer-cli": "1.9.11",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
